@model Team

<table class="table table-bordered">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">TeamName</th>
            <th scope="col">ProjectID</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <th scope="row">@Model.ID</th>
            <td>@Model.TeamName</td>
            <td>@Model.ProjectID</td>
        </tr>
    </tbody>
</table>
